{
  "name": "fatchip-gmbh/oxid-klarna-7",
  "description": "Klarna module for OXID eShop version 7.X",
  "type": "oxideshop-module",
  "version": "7.0.0",
  "license": "Apache-2.2",
  "extra": {
    "oxideshop": {
      "blacklist-filter": [
        "documentation/**/*.*"
